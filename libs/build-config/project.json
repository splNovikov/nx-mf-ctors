{
  "name": "build-config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "root": "libs/build-config",
  "sourceRoot": "libs/build-config/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/build-config",
        "main": "libs/build-config/src/index.ts",
        "tsConfig": "libs/build-config/tsconfig.lib.json",
        "assets": ["libs/build-config/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/build-config/jest.config.ts"
      }
    }
  }
}
